expr: term (PLUS|MINUS) expr
       | UNARY term
term: factor (MULTIPLY|DIVIDE|MODULUS) term
       | factor POWER term
       | UNARY factor
factor: NUMBER | VARIABLE | '(' expr ')'
       | UNARY factor

NUMBER: [0-9]+(\.[0-9]+)?
VARIABLE: [a-zA-Z]+
PLUS: '+'
MINUS: '-'
MULTIPLY: '*'
DIVIDE: '/'
MODULUS: '%'
POWER: '^'
UNARY: ('+'|'-')
